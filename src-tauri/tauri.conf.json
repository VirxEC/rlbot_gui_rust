{
  "package": {
    "productName": "RLBotGUI"
  },
  "build": {
    "distDir": "../assets",
    "devPath": "http://localhost:5500/",
    "withGlobalTauri": true
  },
  "tauri": {
    "allowlist": {
      "shell": {
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "identifier": "org.rlbot.gui",
      "icon": [
        "../icons/icon.png",
        "../icons/icon.ico"
      ],
      "resources": [],
      "externalBin": [],
      "copyright": "MIT",
      "category": "DeveloperTool",
      "shortDescription": "The RLBot GUI",
      "longDescription": "The Rust implementation of RLBot's custom GUI"
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "label": "main",
        "title": "RLBotGUI",
        "width": 1300,
        "height": 870,
        "resizable": true,
        "fullscreen": false,
        "fileDropEnabled": false
      }
    ]
  }
}
